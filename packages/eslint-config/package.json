{
  "name": "@vp-tw/eslint-config",
  "version": "0.1.4",
  "description": "ViPro's ESLint configuration",
  "homepage": "https://github.com/vdustr/eslint-config",
  "author": {
    "name": "ViPro",
    "url": "https://vdustr.dev"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.mjs"
      },
      "default": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    },
    "./README.md": "./README.md",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "copy": "tsx scripts/copy",
    "prepare": "run-s typegen",
    "prepublishOnly": "run-s build copy",
    "typegen": "jiti scripts/typegen"
  },
  "dependencies": {
    "@mui/types": "catalog:",
    "es-toolkit": "catalog:",
    "eslint-flat-config-utils": "catalog:",
    "local-pkg": "catalog:"
  },
  "devDependencies": {
    "eslint-typegen": "catalog:",
    "unbuild": "catalog:"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@emotion/eslint-plugin": "catalog:",
    "@tanstack/eslint-plugin-query": "catalog:",
    "@types/eslint-config-prettier": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-mdx": "catalog:",
    "eslint-plugin-package-json": "catalog:",
    "eslint-plugin-react-compiler": "catalog:",
    "eslint-plugin-storybook": "catalog:",
    "eslint-plugin-yml": "catalog:",
    "storybook": "catalog:"
  },
  "peerDependenciesMeta": {
    "@emotion/eslint-plugin": {
      "optional": true
    },
    "@tanstack/eslint-plugin-query": {
      "optional": true
    },
    "@types/eslint-config-prettier": {
      "optional": true
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-plugin-mdx": {
      "optional": true
    },
    "eslint-plugin-package-json": {
      "optional": true
    },
    "eslint-plugin-react-compiler": {
      "optional": true
    },
    "eslint-plugin-storybook": {
      "optional": true
    },
    "eslint-plugin-yml": {
      "optional": true
    },
    "storybook": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
